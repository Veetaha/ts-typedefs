{
    "name": "ts-typedefs",
    "version": "3.1.0",
    "description": "A handy collection of TypeScript plain and generic type definitions and interfaces.",
    "main": "build/lib/index.js",
    "types": "build/lib/index.d.ts",
    "keywords": [
        "types",
        "typescript",
        "typings",
        "typedefs",
        "interfaces",
        "type definitions",
        "essentials"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Veetaha/ts-typedefs.git"
    },
    "scripts": {
        "pre-deploy": "npm-run-all test build",
        "build": "     npm-run-all clean codegen transpile",
        "test": "      jest",
        "transpile": " tsc",
        "clean": "     rimraf build",
        "codegen": "   node lib/types/eval/_generate.js",
        "bootstrap": " npm i && npm run build",

        "docs": "typedoc --options typedoc.json lib/index.ts && touch docs/.nojekyll && cp assets/* docs/assets/.",
        
        "bump-deps": "ncu -u && npm i"
    },
    "author": "Vitaliy Kryvenko",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^24.0.18",
        "jest": "^24.9.0",
        "npm-check-updates": "^3.1.21",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.0",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tslint": "^5.19.0",
        "typedoc": "^0.15.0",
        "typescript": "^3.5.3"
    },
    "peerDependencies": {
        "typescript": ">=3.6.x"
    }
}

{
    "name": "ts-typedefs",
    "version": "1.0.2",
    "description": "A handy collection of TypeScript plain and generic type definitions and interfaces.",
    "main": "build/lib/index.js",
    "types": "build/lib/index.d.ts",
    "keywords": [
        "types",
        "typescript",
        "typings",
        "typedefs",
        "interfaces",
        "type definitions",
        "essentials"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Veetaha/ts-typedefs.git"
    },
    "scripts": {
        "bootstrap": "npm i && npm run build",
        "build": "npm run codegen && tsc && npm run test",
        "build-prod": "npm run codegen && tsc --project tsconfig.prod.json",
        "clean": "rm -rf build docs",
        "codegen": "ts-node lib/types/eval/_generate.ts",
        "docs": "./node_modules/.bin/typedoc --options typedoc.json lib/index.ts && touch docs/.nojekyll",
        "pre-deploy": "npm run clean && npm run build && npm run test && npm run clean && npm run build-prod",
        "test": "mocha build/test/tests.js",
        "update-deps": "ncu -u && npm i"
    },
    "author": "Vitaliy Kryvenko",
    "license": "MIT",
    "dependencies": {
        "typescript": "^3.2.4"
    },
    "devDependencies": {
        "@types/chai": "latest",
        "@types/mocha": "latest",
        "chai": "latest",
        "mocha": "latest",
        "npm-check-updates": "^3.0.4",
        "ts-node": "^8.0.3",
        "tslint": "^5.13.1",
        "typedoc": "^0.14.2"
    }
}

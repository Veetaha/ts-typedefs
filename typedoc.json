{
    "mode":                           "file",
    "out":                            "docs",
    "exclude":                        "test",
    "theme":                          "default",
    "ignoreCompilerErrors":           true,
    "excludePrivate":                 true,
    "excludeNotExported":             true,
    "target":                         "es2019",
    "moduleResolution":               "node",
    "preserveConstEnums":             true,
    "stripInternal":                  true,
    "suppressExcessPropertyErrors":   true,
    "suppressImplicitAnyIndexErrors": true,
    "module":                         "commonjs"
}

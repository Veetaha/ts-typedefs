{
    "mode":                           "modules",
    "out":                            "docs",
    "exclude":                        "test",
    "theme":                          "default",
    "ignoreCompilerErrors":           true,
    "excludePrivate":                 true,
    "excludeNotExported":             "true",
    "target":                         "ES2018",
    "moduleResolution":               "node",
    "preserveConstEnums":             "true",
    "stripInternal":                  "true",
    "suppressExcessPropertyErrors":   "true",
    "suppressImplicitAnyIndexErrors": "true",
    "module":                         "commonjs"
}